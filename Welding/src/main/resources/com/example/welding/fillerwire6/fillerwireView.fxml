<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.welding.fillerwire6.fillerwireController">
   <children>
      <Label layoutX="159.0" layoutY="66.0" text="Choose the Filler Wire">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button layoutX="399.0" layoutY="323.0" mnemonicParsing="false" onAction="#suggestWire" style="-fx-background-color: #0468BF; -fx-background-radius: 6px; -fx-text-fill: white;" text="I don't know" />
      <ComboBox fx:id="wireCombo" layoutX="230.0" layoutY="200.0" prefWidth="150.0" />
      <Button layoutX="159.0" layoutY="323.0" mnemonicParsing="false" onAction="#next" prefHeight="25.0" prefWidth="82.0" style="-fx-background-color: #0468BF; -fx-background-radius: 6px; -fx-text-fill: white;" text="Next" />
      <Label fx:id="selectText" layoutX="231.0" layoutY="183.0" nodeOrientation="LEFT_TO_RIGHT" text="Please select an item" textFill="#d40e0e" visible="false" />
   </children>
</AnchorPane>
